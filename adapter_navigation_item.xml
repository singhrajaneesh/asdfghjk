<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="200dp"
    android:layout_height="60dp"
    >
    <ImageView
        android:id="@+id/image51"
        android:layout_width="@dimen/adapternavigation_imageview_layout_width"
        android:layout_height="@dimen/adapternavigation_imageview_layout_height"
        android:layout_marginLeft="@dimen/adapternavigation_imageview_layout_marginLeft"
        android:layout_marginRight="@dimen/adapternavigation_imageview_layout_marginRight"
        android:layout_marginBottom="@dimen/adapternavigation_imageview_layout_marginBottom"
        android:layout_centerVertical="true"
        />
    <TextView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/tv131"
        android:layout_marginLeft="@dimen/adapternavigation_textview_layout_marginLeft"
        android:textColor="@color/Black_Dash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/image51"
        />
</RelativeLayout>
